{
  "expo": {
    "slug": "soulbangsapp",
    "ios": {
      "supportsTablet": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0B0F19",
        "foregroundImage": "./assets/icon.png"
      }
    },
    "name": "SoulBangsApp",
    "web": {
      "bundler": "metro"
    },
    "splash": {
      "backgroundColor": "#0B0F19",
      "resizeMode": "contain",
      "image": "./assets/splash.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
